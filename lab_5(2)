#include <iostream>
#include <fstream>
#include <string>
#include <windows.h>
int main()
{
	SetConsoleCP(1251);
	SetConsoleOutputCP(1251);
	setlocale(LC_ALL, "Rus");
	std::ifstream fin("input.txt");
	std::ofstream fout("output.txt");
	std::string str;
	//std::string sogl = "бвгджзклмнпрстйцшщхфч";
	std::string sogl = "qwrtpsdfghjklzxcvbnm";
	std::string word[1000];
	int count[1000];
	int N, size = 0;
	std::cin >> N;

	while (fin >> str)
	{
		int sum = 0;
		for (int i = 0; i < str.length(); i++)
		{
			str[i] = tolower(str[i]);
		}
		while (ispunct(str[0]))
		{
			str.erase(0, 1);
		}
		while (ispunct(str[str.length() - 1]))
		{
			str.erase(str.length() - 1);
		}
		for (int i = 0; i < str.length(); i++)
		{
			if (sogl.find(str[i]) >= 0 && sogl.find(str[i]) < sogl.length() && sogl.find(str[i + 1]) >= 0 && sogl.find(str[i + 1]) < sogl.length())
			{
				sum += 1;
			}
		}
		if (sum > 0)
		{
			bool flag = true;
			for (int i = 0; i < size; i++)
			{
				if (str == word[i])
				{
					flag = false;
				}
			}
			if (flag == true)
			{
				word[size] = str;
				count[size] = sum;
				size += 1;
			}
		}
	}

	for (int i = 0; i < size - 1; i++)
	{
		for (int j = i + 1; j < size; j++)
		{
			if (count[i] < count[j])
			{
				std::swap(word[i], word[j]);
				std::swap(count[i], count[j]);
			}
		}
	}
	if (N > size)
	{
		std::cout << "Нашлось слов: " << size << std::endl;
		N = size;
	}

	for (int i = 0; i < N; i++)
	{
		fout << word[i] << " " << count[i] << std::endl;
	}

	system("pause");
}
