#include <iostream>

int main()
{
	int n, m, max, ind;
	max = INT_MIN;
	ind = -1;
	std::cin >> n;
	std::cin >> m;
	int matrix[101][101];
	int mult[101];
	for (int i = 0; i < n; i++)
	{
		mult[i] = 1;
		for (int j = 0; j < m; j++)
		{
			std::cin >> matrix[i][j];
			mult[i] *= matrix[i][j];
		}
		if (mult[i] > max)
		{
			max = mult[i];
			ind = i;
		}
	}
		
	for (int j = 0; j < m; j++)
	{
		matrix[ind][j] = max;
	}

	for (int i = 0; i < n; i++)
	{
		for (int j = 0; j < m; j++)
			std::cout << matrix[i][j] << " ";
		std::cout << std::endl;
	}
	system("pause");
	return 0;
}
