#include <iostream>

int main()
{
	unsigned int a;
	std::cin >> a;
	unsigned char* firstByte = (unsigned char*)&a;
	std::cout << (unsigned int)*firstByte << " " << (unsigned int)*(firstByte + 1) << " " << (unsigned int)*(firstByte + 2) << " " << (unsigned int)*(firstByte + 3) << std::endl;
	system("pause");
}
