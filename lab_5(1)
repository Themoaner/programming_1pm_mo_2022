#include <iostream>

int main()
{
	char a;
	char str[101];
	bool flag = false;
	std::cin >> str;
	std::cin >> a;
	for (int i = 0; i < strlen(str); i++)
	{
		if (a == str[i])
		{
			std::cout << i << std::endl;
			flag = true;
		}
	}
	if (flag == false)
	{
		std::cout << "no such symbol" << std::endl;
	}
	system("pause");
	return 0;
}
