#include <iostream>
#include "Header.h"

namespace sk {
    void read(int& n, int& m, int matrix[N][M])
    {
        std::cin >> n >> m;
        for (int i = 0; i < n; i++)
            for (int j = 0; j < m; j++)
            {
                std::cin >> matrix[i][j];
            }
    }

    bool primeNumbers(int n, int m, int matrix[N][M])
    {
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++)
            {
                bool isPrime = true;
                for (int d = 2; d <= sqrt(matrix[i][j]); d++)
                {
                    if (matrix[i][j] % d == 0)
                    {
                        isPrime = false;
                        break;
                    }
                }
            }
    }

    int maxInMatrix(int n, int m, int matrix[N][M])
    {
        int max = INT_MIN;
        int count = 0;
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++)
            {
                if (matrix[i][j] > max)
                {
                    max = matrix[i][j];
                }
            }
        }
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++)
            {
                if (matrix[i][j] == max)
                {
                    count++;
                }
            }
        }
        if (count == 2)
        {

        }
    }

    void write(int n, int m, int matrix[N][M])
    {
        std::cout << std::endl;
        
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < m; j++)
            {
                std::cout << matrix[i][j] << " ";
            }
            std::cout << std::endl;
        }
    }
}
